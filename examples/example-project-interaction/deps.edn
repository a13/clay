{:deps {clojure2d/clojure2d {:mvn/version "1.4.4"}
        scicloj/tablecloth {:mvn/version "6.076"}
        org.scicloj/viz.clj {:mvn/version "1-alpha3"}
        org.scicloj/clay {:mvn/version "1-alpha9"}}
 :paths ["src"]
 :aliases {:clay
           {:extra-deps {cider/cider-nrepl {:mvn/version "0.28.3"}}
